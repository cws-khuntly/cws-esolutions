<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <interactiveMode>true</interactiveMode>
    <offline>false</offline>

    <servers>
        <server>
            <id>artifactory-ext-libs</id>
        </server>
        <server>
            <id>artifactory-int-libs</id>
        </server>
        <server>
            <id>artifactory-release</id>
        </server>
        <server>
            <id>artifactory-snapshots</id>
        </server>
    </servers>

    <profiles>
        <profile>
            <id>global</id>
            <properties>
                <downloadSources>true</downloadSources>
                <downloadJavadoc>true</downloadJavadoc>
            </properties>
            <repositories>
                <repository>
                    <id>artifactory-release</id>
                    <name>Artifactory Managed Internal Repository</name>
                    <url>http://repository.caspersbox.corp:8081/artifactory/release</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>artifactory-snapshots</id>
                    <name>Artifactory Managed Internal Repository</name>
                    <url>http://repository.caspersbox.corp:8081/artifactory/snapshots</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>artifactory-ext-libs</id>
                    <name>Artifactory Managed Internal Repository</name>
                    <url>http://repository.caspersbox.corp:8081/artifactory/ext-libs</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>artifactory-int-libs</id>
                    <name>Artifactory Managed Internal Repository</name>
                    <url>http://repository.caspersbox.corp:8081/artifactory/int-libs</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>central</id>
                    <url>http://repository.caspersbox.corp:8081/artifactory/central</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>repo1</id>
                    <url>http://repository.caspersbox.corp:8081/artifactory/repo1</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>codehaus</id>
                    <url>http://repository.caspersbox.corp:8081/artifactory/codehaus</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>EclipseLink</id>
                    <url>http://repository.caspersbox.corp:8081/artifactory/EclipseLink</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>central</id>
                    <url>http://repository.caspersbox.corp:8081/artifactory/central</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>repo1</id>
                    <url>http://repository.caspersbox.corp:8081/artifactory/repo1</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>codehaus</id>
                    <url>http://repository.caspersbox.corp:8081/artifactory/codehaus</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>global</activeProfile>
    </activeProfiles>
</settings>
