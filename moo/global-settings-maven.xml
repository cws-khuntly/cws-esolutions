<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <interactiveMode>true</interactiveMode>
    <offline>false</offline>

    <mirrors>
        <mirror>
            <id>ext-libs</id>
            <url>http://repository.caspersbox.corp:8081/archiva/repository/ext-libs/</url>
            <mirrorOf>*,!internal,!snapshots,!ext-libs</mirrorOf>
        </mirror>
    </mirrors>

    <servers>
        <server>
            <id>internal</id>
            <filePermissions>644</filePermissions>
            <directoryPermissions>755</directoryPermissions>
            <configuration>
                <timeout>10</timeout>
            </configuration>
        </server>
        <server>
            <id>snapshots</id>
            <filePermissions>644</filePermissions>
            <directoryPermissions>755</directoryPermissions>
            <configuration>
                <timeout>1</timeout>
            </configuration>
        </server>
        <server>
            <id>ext-libs</id>
            <filePermissions>644</filePermissions>
            <directoryPermissions>755</directoryPermissions>
            <configuration>
                <timeout>1</timeout>
            </configuration>
        </server>
    </servers>

    <profiles>
        <profile>
            <id>global</id>
            <properties>
                <downloadSources>true</downloadSources>
                <downloadJavadoc>true</downloadJavadoc>
                <maven.build.timestamp.format>dd MMM yyyy HH:mm:ss:SS z (Z)</maven.build.timestamp.format>
                <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
                <sourceReleaseAssemblyDescriptor>source-release</sourceReleaseAssemblyDescriptor>
                <cws.maven.repository>http://repository.caspersbox.corp:8081/artifactory</cws.maven.repository>
                <cws.scm.issues.url>https://github.com/cws-khuntly/cws-esolutions/issues</cws.scm.issues.url>
                <cws.ci.url>http://repository.caspersbox.corp:8080/</cws.ci.url>
                <cws.scm.connection>scm:git:https://github.com/cwsus/cws-esolutions.git</cws.scm.connection>
                <cws.scm.developerConnection>scm:git:git@github.com:cwsus/cws-esolutions.git</cws.scm.developerConnection>
                <cws.scm.tag>HEAD</cws.scm.tag>
            </properties>
            <repositories>
                <repository>
                    <id>internal</id>
                    <name>Archiva Managed Internal Repository</name>
                    <url>http://repository.caspersbox.corp:8081/archiva/repository/internal</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>snapshots</id>
                    <name>Archiva Managed Internal Repository</name>
                    <url>http://repository.caspersbox.corp:8081/archiva/repository/snapshots</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>ext-libs</id>
                    <name>Archiva Managed Internal Repository</name>
                    <url>http://repository.caspersbox.corp:8081/archiva/repository/ext-libs</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories />
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>global</activeProfile>
    </activeProfiles>
</settings>
