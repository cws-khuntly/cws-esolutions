<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cws.esolutions.android.ui"
    android:versionCode="1"
    android:versionName="1.0"
    android:installLocation="auto">

    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="1" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <instrumentation android:name="android.test.InstrumentationTestRunner"
        android:targetPackage="com.cws.esolutions.android"
        android:label="junit"
        android:functionalTest="true"
        android:handleProfiling="true" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/logo"
        android:label="@string/appName"
        android:theme="@style/global"
        android:allowClearUserData="true"
        android:logo="@drawable/logo">

        <uses-library android:name="android.test.runner" />

        <activity android:name="com.cws.esolutions.android.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cws.esolutions.android.ui.ApplicationManagementActivity" />
        <activity android:name="com.cws.esolutions.android.ui.DNSServiceActivity" />
        <activity android:name="com.cws.esolutions.android.ui.HomeActivity" />
        <activity android:name="com.cws.esolutions.android.ui.LoginActivity" />
        <activity android:name="com.cws.esolutions.android.ui.OnlineResetActivity" />
        <activity android:name="com.cws.esolutions.android.ui.ServiceManagementActivity" />
        <activity android:name="com.cws.esolutions.android.ui.ServiceMessagingActivity" />
        <activity android:name="com.cws.esolutions.android.ui.SystemCheckActivity" />
        <activity android:name="com.cws.esolutions.android.ui.SystemManagementActivity" />
        <activity android:name="com.cws.esolutions.android.ui.UserAccountActivity" />
        <activity android:name="com.cws.esolutions.android.ui.VirtualManagerActivity" />
    </application>
</manifest>
