<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application-config>
        <application-config key="charset">UTF-8</application-config>
        <application-config key="applicationName">esolutions</application-config>
        <application-config key="dateFormat">MM/dd/yyyy hh:mm</application-config>
        <application-config key="proxyConfigFile">eSolutions/config/httpConfig/ProxyConfig.xml</application-config>
        <application-config key="httpConfigFile">eSolutions/config/httpConfig/HTTPConfig.xml</application-config>
    </application-config>

    <dns-config>
        <dns key="ServiceURL">http://webtools.live2support.com/nt_digajax.php?queryType=dig&amp;target={0}&amp;record={1}</dns>
        <dns key="SearchSuffix">caspersbox.corp, caspersbox.com</dns>
    </dns-config>
    
    <services>
        <service key="kbase">true</service>
        <service key="siteSearch">true</service>
        <service key="dnsAdmin">true</service>
        <service key="serviceRequest">true</service>
        <service key="userAdmin">true</service>
        <service key="appMgmt">true</service>
        <service key="deployApp">true</service>
        <service key="buildApp">true</service>
    </services>

    <service-config>
        <service key="target-port">4444</service>
    </service-config>

    <connection-factory-config>
        <connection-name>ApplicationDataSourceManager</connection-name>
        <ApplicationDataSourceManager>
            <connection key="datasource">jdbc:mysql://data.caspersbox.corp/esolutions</connection>
            <connection key="driver">com.mysql.jdbc.Driver</connection>
            <connection key="username">appuser</connection>
            <connection key="password">appuser10</connection>
        </ApplicationDataSourceManager>
    </connection-factory-config>
</config>